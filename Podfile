target 'RunRRR 2.0' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'GoogleMaps'
  pod 'SnapKit'
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON', '~> 4.0'  
  pod 'SWXMLHash', '~> 4.0.0'

  target 'RunRRR 2.0Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'RunRRR 2.0UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
